    <script type="text/javascript">
        var isHidden = false;
        window.onclick = function () {
            if (isHidden) {
                document.getElementById('PanelLeft').className = '';
                document.getElementById('PanelRight').className = '';
                isHidden = false;
            } else {
                document.getElementById('PanelLeft').className = 'move-left';
                document.getElementById('PanelRight').className = 'move-right';
                isHidden = true;
            }
        }
    </script>
